{
  "name": "storybook-react-typescript",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@craco/craco": "^6.3.0",
    "@emotion/core": "^11.0.0",
    "@emotion/css": "^11.1.3",
    "@emotion/react": "^11.7.0",
    "@emotion/styled": "^11.3.0",
    "@fortawesome/fontawesome-svg-core": "^1.2.36",
    "@fortawesome/free-solid-svg-icons": "^5.15.4",
    "@fortawesome/react-fontawesome": "^0.1.16",
    "@react-theming/storybook-addon": "^1.1.3",
    "@testing-library/react": "^11.1.0",
    "@testing-library/user-event": "^12.1.10",
    "@types/jest": "^27.0.3",
    "@types/node": "^16.10.4",
    "@types/react": "^17.0.29",
    "@types/react-dom": "^17.0.9",
    "axios": "^0.24.0",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-icons": "^4.3.1",
    "react-router-dom": "^6.0.2",
    "react-scripts": "4.0.3",
    "recoil": "^0.5.2",
    "start-server-and-test": "^1.14.0",
    "typescript": "^4.1.2"
  },
  "scripts": {
    "start": "yarn craco start",
    "start-dev": "craco start",
    "build": "craco build",
    "test": "craco test -u",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 6005 -s public",
    "build-storybook": "build-storybook -s public",
    "test:coverage": "yarn test --watchAll=false --coverage",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true",
    "cypress:start:app": "cross-env BROWSER=none react-scripts -r @cypress/instrument-cra start",
    "cypress:start:wait": "start-server-and-test cypress:start:app http://localhost:3000",
    "cypress:open": "yarn cypress:start:wait -- \"cypress open\"",
    "cypress:run": "yarn cypress:start:wait -- \"cypress run\""
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ],
    "overrides": [
      {
        "files": [
          "**/*.stories.*"
        ],
        "rules": {
          "import/no-anonymous-default-export": "off"
        }
      }
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.16.5",
    "@babel/preset-env": "^7.16.5",
    "@babel/preset-typescript": "^7.16.5",
    "@cypress/code-coverage": "^3.9.12",
    "@cypress/instrument-cra": "^1.4.0",
    "@cypress/react": "^5.10.3",
    "@cypress/webpack-dev-server": "^1.7.0",
    "@storybook/addon-actions": "^6.3.11",
    "@storybook/addon-essentials": "^6.3.11",
    "@storybook/addon-jest": "^6.4.9",
    "@storybook/addon-links": "^6.3.11",
    "@storybook/addon-storyshots": "^6.4.9",
    "@storybook/node-logger": "^6.3.11",
    "@storybook/preset-create-react-app": "^3.2.0",
    "@storybook/react": "^6.3.11",
    "@testing-library/cypress": "^8.0.2",
    "@testing-library/jest-dom": "^5.16.1",
    "@types/lodash": "^4.14.176",
    "@typescript-eslint/eslint-plugin": "^4.32.0",
    "@typescript-eslint/parser": "^4.32.0",
    "babel-jest": "^27.4.5",
    "craco-alias": "^3.0.1",
    "cross-env": "^7.0.3",
    "cypress": "^9.1.1",
    "cypress-react-selector": "^2.3.13",
    "eslint": "^7.32.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-config-react-app": "^6.0.0",
    "eslint-loader": "^4.0.2",
    "eslint-plugin-cypress": "^2.12.1",
    "eslint-plugin-import": "^2.24.2",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.26.1",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.4.1",
    "react-test-renderer": "^17.0.2"
  },
  "resolutions": {
    "babel-loader": "8.1.0",
    "webpack": "4.44.2"
  }
}
